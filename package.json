{
  "name": "my-antd-ts-app",
  "version": "0.1.0",
  "private": true,
  "host": "0.0.0.0",
  "port": "3300",
  "dependencies": {
    "antd": "^2.13.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts-ts-antd": "0.4.8"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,less}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts-ts-antd start",
    "build": "react-scripts-ts-antd build",
    "precommit": "lint-staged",
    "test": "react-scripts-ts-antd test --env=jsdom",
    "eject": "react-scripts-ts-antd eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "analyze": "source-map-explorer build/static/js/main.*",
    "web:s": "serve -s build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d master build"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "^1.8.2",
    "source-map-explorer": "^1.5.0"
  }
}
